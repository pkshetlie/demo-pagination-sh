    <title>DEMO</title>
</head>
<body>
<div class="container">
    {% embed "pagination-table.html.twig" with {pagination: songs} %}
        {% block thead %}
            <tr>
                <th>{{ pagination|order_link('title', 'Title') }}</th>
                <th>Author</th>
                <th>{{ pagination|order_link('mapper', 'Mapper') }}</th>
            </tr>
        {% endblock %}
        {% block tbody %}
            {% include 'partial/rows.html.twig' with {songs: songs} %}
        {% endblock %}
        {% block pagination %}
            {{ parent() }}
            et du texte en plus
        {% endblock %}
    {% endembed %}
</div>
</body>
</html>

